package locators;

import org.openqa.selenium.By;
import org.openqa.selenium.chrome.ChromeDriver;

public class GmailLogin {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		
		System.setProperty("webdriver.chrome.driver","C:/Softwares/Extracted Files/chromedriver_win32 new/chromedriver.exe");
		ChromeDriver driver = new ChromeDriver();

		driver.manage().window().maximize();
			
		driver.get("https://accounts.google.com/signin/v2/identifier?continue=https%3A%2F%2Fmail.google.com%2Fmail%2F&service=mail&sacu=1&rip=1&flowName=GlifWebSignIn&flowEntry=ServiceLogin");
		driver.findElement(By.xpath("identifierId")).sendKeys("rohith555raju@gmail.com");
		Thread.sleep(300);
		driver.findElement(By.id("ZFr60d CeoRYc")).click();
		Thread.sleep(300);
		driver.findElement(By.xpath("/html/body/div[1]/div[1]/div[2]/div/div[2]/div/div[2]/div/form/div[1]/div/div[1]/div/div[1]/input")).sendKeys("ilmyammananna!123#");
		driver.findElement(By.className("ZFr60d CeoRYc")).click();
		
		//WebDriverWait wait = new WebDriverWait(driver,20);
		//wait.until(ExpectedConditions.visibilityOfElementLocated(By.id("logoutLink")));
		//String title = driver.getTitle();
		//System.out.println(title);
		
		driver.close();	

		

	}

}
